<!DOCTYPE html>
<html>
  <head>
    <title>Travelr</title>
    <%= csrf_meta_tags %>
    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
    <!--Let browser know website is optimized for mobile-->
    <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
    <script src="//maps.google.com/maps/api/js?key=AIzaSyAV7074pRq1reMMYAn96Qg5eAD9z0NYqw0"></script>
    <script src="//cdn.rawgit.com/mahnunchik/markerclustererplus/master/dist/markerclusterer.min.js"></script>
    <script src='//cdn.rawgit.com/printercu/google-maps-utility-library-v3-read-only/master/infobox/src/infobox_packed.js' type='text/javascript'></script>
    <!-- only if you need custom infoboxes -->

  </head>
  <body>
    <% if @current_user.present? %>
    <%= render partial: 'layouts/nav' %>
    <% end %>
    <div style='width: 800px;'>
      <div id="map" style='width: 800px; height: 400px;'></div>
    </div>
    <!-- <input id="input1" class="controls" type="text" placeholder="Search Address"> -->
    <!-- <button id="btn-search">Search Address</button> -->
    <!-- <input id="input2" class="controls" type="text" placeholder="Search Postcode">
    <button>Search Postcode</button> -->
    <%= yield %>
  </body>
</html>
