<div class="container">
    <h2>Spots Index</h2>
    <hr>
    <ul>
      <% @spots.each do |spot| %>
        <li>
          <span><%= spot.title %> </span>
          <%= link_to "Show", spot %>
          <%= link_to 'Edit', edit_spot_path(spot) %>
          <%= link_to 'Destroy', spot, method: :delete, data: { confirm: 'Are you sure?' } %>
        </li>
      <% end %>
    </ul>
</div>
